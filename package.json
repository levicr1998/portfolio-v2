{
  "name": "portfolio-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "codegen": "eval $(grep '^VITE_CONTENTFUL_SPACE_ID' .env) && eval $(grep '^CONTENTFUL_MANAGEMENT_API_ACCESS_TOKEN' .env) && cf-content-types-generator --spaceId=$VITE_CONTENTFUL_SPACE_ID  --token=$CONTENTFUL_MANAGEMENT_API_ACCESS_TOKEN -o src/types"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
